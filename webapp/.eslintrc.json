{
    "extends": "airbnb",
    "rules": {
        "comma-dangle": ["error", "never"],
        "indent": ["error", 4],
        "max-len": ["warn", 120],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "object-shorthand": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/prefer-stateless-function": 0
    }
}